func _on_l_submit_button_pressed():
	if GlobalVars.cur_l_plotpoint == \"L_PP1\" and GlobalVars.l_pp1_solved == false:
		if check_L_PP1_success():
			GlobalVars.l_pp1_solved = true
			l_select_button1.disabled = true
			l_select_button2.disabled = false
			reset_ldb_text()
			deallocate_lpp1()
		else:
			reset_ldb_text()
			set_lpp1_pos()
			
			
	elif GlobalVars.cur_l_plotpoint == \"L_PP2\" and GlobalVars.l_pp2_solved == false:
		if check_L_PP2_success():
			GlobalVars.l_pp2_solved = true
			l_select_button2.disabled = true
			#l_select_button3.disabled = false
			deallocate_lpp2()
			reset_ldb_text()
		else:
			reset_lpp2_names()
			set_lpp2_pos()
			reset_ldb_text()
	# @ Todo 
	elif GlobalVars.cur_l_plotpoint == \"L_PP3\" and GlobalVars.l_pp3_solved == false:
		if check_L_PP3_success():
			GlobalVars.l_pp3_solved = true
			l_select_button3.disabled = true
	elif GlobalVars.cur_l_plotpoint == \"L_PP4\"and GlobalVars.l_pp4_solved == false:
		if check_L_PP4_success():
			GlobalVars.l_pp4_solved = true
			l_select_button4.disabled = true

func _on_r_submit_button_pressed():
	if GlobalVars.cur_r_plotpoint == \"R_PP1\" and GlobalVars.r_pp1_solved == false:
		if check_R_PP1_success():
			GlobalVars.r_pp1_solved = true
			r_select_button1.disabled = true
			r_select_button2.disabled = false
			deallocate_rpp1()
			reset_rdb_text()
		else:
			reset_rpp1_names()
			set_rpp1_pos()
			reset_rdb_text()
	elif GlobalVars.cur_r_plotpoint == \"R_PP2\" and GlobalVars.r_pp1_solved == true and GlobalVars.r_pp2_solved == false:
		if check_R_PP2_success():
			GlobalVars.r_pp2_solved = true
			r_select_button2.disabled = true
			#r_select_button3.disabled = false
			deallocate_rpp2()
			reset_rdb_text()
		else:
			reset_rpp2_names()
			set_rpp2_pos()
			reset_rdb_text()
	elif GlobalVars.cur_r_plotpoint == \"R_PP3\":
		pass
	elif GlobalVars.cur_r_plotpoint == \"R_PP4\":
		pass

